{
    "name": "sofwar/slack-laravel",
    "description": "Laravel 4 and 5 integration for the sofwar/slack package, including facades and service providers.",
    "keywords": [
        "slack",
        "laravel"
    ],
    "license": "BSD-2-Clause",
    "authors": [
        {
            "name": "maknz",
            "email": "github@mak.geek.nz"
        },
        {
            "name": "sofwar",
            "email": "ya.sofwar@yandex.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "maknz/slack": "@dev-master",
        "guzzlehttp/guzzle": "6.3.*"
    },
    "autoload": {
        "psr-4": {
            "SofWar\\Slack\\Laravel\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "aliases": {
                "Slack": "SofWar\\Slack\\Laravel\\Facade"
            },
            "providers": [
                "SofWar\\Slack\\Laravel\\ServiceProviderLaravel5"
            ]
        }
    },
    "minimum-stability": "dev"
}
